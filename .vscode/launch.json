    "version": "0.20.25",
    "configurations": [
        {
            "request": "launch",
            "name": "Test File with Deno",
            "type": "node",
            "program": "${workspaceFolder}/${relativeFile}",
            "cwd": "${workspaceFolder}/${relativeFileDirname}",
            "runtimeExecutable": "deno",
            "runtimeArgs": ["run", "--allow-all", "--inspect-brk=127.0.0.1:9229"],
            "attachSimplePort": 9229,
            "outputCapture": "std",
            "args": ["test"]
        },
        {
            "request": "launch",
            "name": "Prod File with Deno",
            "type": "node",
            "program": "${workspaceFolder}/${relativeFile}",
            "cwd": "${workspaceFolder}/${relativeFileDirname}",
            "runtimeExecutable": "deno",
            "runtimeArgs": ["run", "--allow-all", "--inspect-brk=127.0.0.1:9229"],
            "attachSimplePort": 9229,
            "outputCapture": "std",
            "args": ["prod"]
        }
    ]
}
